{
  "name": "monorepo",
  "packageManager": "yarn@4.0.2",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "yarn workspaces foreach -A -p -v -i run dev",
    "build": "yarn workspaces foreach -A -v -i run build",
    "start": "yarn workspaces foreach -A -p -v -i run start"
  }
}
