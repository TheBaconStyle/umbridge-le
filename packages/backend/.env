PORT=8000 # backend will start at this port
DB_HOST=localhost # backend db host
DB_PORT=5432 # backend db port
DB_USERNAME=postgres # backend db username
DB_PASSWORD=postgres # backend db password
DB_NAME=umbridge # backend db name
DB_SYNC=false # this will allow backend autosync changes in db
DB_ENTITIES=dist/**/*.entity.js # relative to backend folder
DB_MIGRATIONS=dist/migrations/*.js # relative to backend folder
#
SALT_ROUNDS=12 # The number of rounds for user password hashing